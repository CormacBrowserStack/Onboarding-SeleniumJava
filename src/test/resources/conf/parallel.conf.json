{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "bstack:options": {
      "buildName": "BrowserStack 2",
      "sessionName": "Cormac Amazon Build",
      "browserVersion": "latest",
      "debug": true
    }
  },

  "environments": {
    "env1": {
        "browserName": "chrome"
    },
    "env2": {
        "browserName": "firefox"
    },
    "env3": {
      "bstack:options": {
        "os": "OS X"
      },
      "browserName": "safari"
    },
    "env4": {
      "browserName": "edge"
    }
  }
}